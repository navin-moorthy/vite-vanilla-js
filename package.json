{
	"private": true,
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"clean:dist": "npx rimraf dist",
		"dev": "vite --host",
		"fix": "npm run fix:eslint && npm run fix:prettier",
		"fix:eslint": "eslint --fix .",
		"fix:prettier": "prettier --write --cache --cache-location=.prettiercache --list-different --ignore-unknown . ",
		"lint": "npm run lint:eslint && npm run lint:prettier",
		"lint:eslint": "eslint .",
		"lint:prettier": "prettier --check --cache --cache-location=.prettiercache --ignore-unknown . ",
		"preview": "vite preview",
		"update:node-version-support": "npx installed-check --fix",
		"updateDeps": "npx npm-check-updates -u --target patch",
		"updateDeps:check": "npx npm-check-updates --interactive --format group --target patch"
	},
	"devDependencies": {
		"@angular-eslint/template-parser": "19.0.2",
		"@eslint/js": "9.18.0",
		"@ianvs/prettier-plugin-sort-imports": "4.4.1",
		"@vitejs/plugin-legacy": "6.0.0",
		"autoprefixer": "10.4.20",
		"cssnano": "7.0.6",
		"eslint": "9.18.0",
		"eslint-config-flat-gitignore": "1.0.0",
		"eslint-config-prettier": "10.0.1",
		"eslint-plugin-promise": "7.2.1",
		"eslint-plugin-tailwindcss": "3.18.0",
		"eslint-plugin-unicorn": "56.0.1",
		"globals": "15.14.0",
		"postcss": "8.5.1",
		"prettier": "3.4.2",
		"prettier-plugin-packagejson": "2.5.8",
		"prettier-plugin-tailwindcss": "0.6.10",
		"tailwindcss": "3.4.17",
		"terser": "5.37.0",
		"vite": "6.0.10",
		"vite-plugin-minify": "^2.1.0"
	},
	"packageManager": "pnpm@9.15.4",
	"engines": {
		"node": "^18.18.0 || ^20.9.0 || >=22.0.0"
	}
}
